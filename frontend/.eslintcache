nent={LandingScreen} exact  />\n      <Route path='/photographs' component={CarouselScreen}  />\n      <Route path='/product/:id' component={ProductScreen}  />\n      <Route path=\"/productlist\" component={ImageScreen}  />\n      <Route path=\"/product/:id/edit\"  component={ImageEditScreen} />\n      <Route path='/login' component={LoginScreen}  />\n      <SideNav />\n      \n      <Route path='/home' component={HomeScreen} exact  />\n      <Route path='/contact' component={ContactScreen} exact  />\n      </main>\n      </Router>\n  )\n}\n\nexport default App;\n",["120","121"],"/Users/Tim/Desktop/Bell/frontend/src/components/Header.js",[],["122","123"],"/Users/Tim/Desktop/Bell/frontend/src/screens/ContactScreen.js",[],["124","125"],"/Users/Tim/Desktop/Bell/frontend/src/screens/CarouselScreen.js",[],"/Users/Tim/Desktop/Bell/frontend/src/products.js",[],"/Users/Tim/Desktop/Bell/frontend/src/index.js",[],"/Users/Tim/Desktop/Bell/frontend/src/store.js",[],"/Users/Tim/Desktop/Bell/frontend/src/screens/PrintScreen.js",[],["126","127"],"/Users/Tim/Desktop/Bell/frontend/src/screens/HomeScreen.js",[],"/Users/Tim/Desktop/Bell/frontend/src/components/Product.js",[],"/Users/Tim/Desktop/Bell/frontend/src/screens/ProductScreen.js",[],"/Users/Tim/Desktop/Bell/frontend/src/reducers/productReducers.js",[],"/Users/Tim/Desktop/Bell/frontend/src/actions/productActions.js",[],"/Users/Tim/Desktop/Bell/frontend/src/constants/productConstants.js",[],"/Users/Tim/Desktop/Bell/frontend/src/components/Message.js",[],["128","129"],"/Users/Tim/Desktop/Bell/frontend/src/components/Loader.js",[],["130","131"],"/Users/Tim/Desktop/Bell/frontend/src/screens/LandingScreen.js",[],"/Users/Tim/Desktop/Bell/frontend/src/components/SideNav.js",[],"/Users/Tim/Desktop/Bell/frontend/src/reducers/userReducers.js",[],["132","133"],"/Users/Tim/Desktop/Bell/frontend/src/screens/LoginScreen.js",["134","135","136"],"/Users/Tim/Desktop/Bell/frontend/src/actions/userActions.js",[],"/Users/Tim/Desktop/Bell/frontend/