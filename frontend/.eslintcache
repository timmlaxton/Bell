n      <Header/>\n      <main className='py-3'> \n      <Route path='/' component={LandingScreen} exact  />\n      <Route path='/photographs' component={CarouselScreen}  />\n      <Route path='/product/:id' component={ProductScreen}  />\n      <Route path=\"/productlist\" component={ImageScreen}  />\n      <Route path=\"/product/:id/edit\"  component={ImageEditScreen} />\n      <Route path='/login' component={LoginScreen}  />\n      <SideNav />\n      \n      <Route path='/home' component={HomeScreen} exact  />\n      <Route path='/contact' component={ContactScreen} exact  />\n      </main>\n      </Router>\n  )\n}\n\nexport default App;\n","/Users/Tim/Desktop/Bell/frontend/src/components/Header.js",[],["119","120"],"/Users/Tim/Desktop/Bell/frontend/src/screens/ContactScreen.js",[],"/Users/Tim/Desktop/Bell/frontend/src/screens/CarouselScreen.js",[],"/Users/Tim/Desktop/Bell/frontend/src/products.js",[],"/Users/Tim/Desktop/Bell/frontend/src/index.js",[],"/Users/Tim/Desktop/Bell/frontend/src/store.js",[],"/Users/Tim/Desktop/Bell/frontend/src/screens/PrintScreen.js",[],["121","122"],"/Users/Tim/Desktop/Bell/frontend/src/screens/HomeScreen.js",[],"/Users/Tim/Desktop/Bell/frontend/src/components/Product.js",[],["123","124"],"/Users/Tim/Desktop/Bell/frontend/src/screens/ProductScreen.js",[],"/Users/Tim/Desktop/Bell/frontend/src/reducers/productReducers.js",[],"/Users/Tim/Desktop/Bell/frontend/src/actions/productActions.js",[],["125","126"],"/Users/Tim/Desktop/Bell/frontend/src/constants/productConstants.js",[],"/Users/Tim/Desktop/Bell/frontend/src/components/Message.js",[],["127","128"],"/Users/Tim/Desktop/Bell/frontend/src/components/Loader.js",[],"/Users/Tim/Desktop/Bell/frontend/src/screens/LandingScreen.js",[],"/Users/Tim/Desktop/Bell/frontend/src/components/SideNav.js",[],"/Users/Tim/Desktop/Bell/frontend/src/reducers/userReducers.js",[],"/Users/Tim/Desktop/Bell/frontend/src/screens/LoginScreen.js",["129","130","131"],"import React,{useState, useEffect} from 'react'\nimport {Link} from 'react-router-dom'\nimport {Form, Button, Row, Col} from 'react-bootstrap'\nimport {useDispatch, useSelector} from 'react-redux'\nimport Message from '../components/Message'\nimport Loader from '../components/Loader'\nimport FormContainer from '../components/FormContainer'\nimport {login} from '../actions/userActions'\n\n\n\nconst LoginScreen = ({location, history}) => {\n  const [email, setEmail] = useState('')\n  const [password, setPassword] = useState('')\n\n  const dispatch = useDispatch()\n\n  const userLogin = useSelector(state => state.userLogin)\n  const {loading, error, userInfo} = userLogin\n\n  const redirect = location.search ? location.search.split('=')[1] : '/home'\n\n\n  useEffect(() => {\n    if(userInfo) {\n      history.push(redirect)\n    }\n  }, [ history, userInfo, redirect])\n\n  const submitHandler = (e) => {\n    e.preventDefault()\n    dispatch(login(email, password))\n  }\n\n\n  return (\n    <FormContainer>\n      <h1>Sign In</h1>\n      {error && <Message variant='danger'>{error}</Message>}\n      {loading && <Loader/>}\n      <Form onSubmit={submitHandler}>\n        <Form.Group controlId='email'>\n          <Form.Label>Email Address</Form.Label>\n          <Form.Control \n          type='email' \n          placeholder=\"Enter email\" \n          value={email} \n          onChange={(e) => setEmail(e.target.value)}\n          ></Form.Control>\n        </Form.Group>\n\n        <Form.Group controlId='password'>\n          <Form.Label>Password</Form.Label>\n          <Form.Control \n          type='password' \n          placeholder=\"Enter password\" \n          value={password} \n          onChange={(e) => setPassword(e.target.value)}\n          ></Form.Control>\n        </Form.Group>\n\n        <Button type='submit' variant='primary'>\n          Sign In\n        </Button>\n      </Form>      \n    </FormContainer>\n  )\n}\n\nexport default LoginScreen\n ","/Users/Tim/Desktop/Bell/frontend/src/actions/userActions.js",[],"